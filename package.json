{
	"name": "codeforces-tool",
	"displayName": "codeforces-tool",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cf.setup",
		"onCommand:cf.setusername",
		"onCommand:cf.setpassword"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cf.setup",
				"title": "setup",
				"category": "Codeforces"
			},
			{
				"command": "cf.setusername",
				"title": "set username",
				"category": "Codeforces"
			},
			{
				"command": "cf.setpassword",
				"title": "set password",
				"category": "Codeforces"
			}
		],
		"configuration": {
      "title": "Codeforces",
      "properties": {
        "Codeforces.username": {
          "type": "string",
          "default": "",
          "description": "your username"
				},
				"Codeforces.password": {
          "type": "string",
          "default": "",
          "description": "your password"
        }
      }
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
